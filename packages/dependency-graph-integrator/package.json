		"test": "node --import tsx --test \"**/*.test.ts\"",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@aws-sdk/client-sns": "^3.958.0",
		"ts-markdown": "^1.2.0",
		"yaml": "^2.8.2"
	}
}