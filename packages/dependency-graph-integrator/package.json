{
	"name": "dependency-graph-integrator",
	"version": "1.0.0",
	"type": "module",
	"devDependencies": {
		"@types/aws-lambda": "^8.10.159"
	},
	"scripts": {
		"build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outdir=dist --external:@aws-sdk && cp ../../.github/workflows/template_dependency_submission.yaml dist/template_dependency_submission.yaml",
